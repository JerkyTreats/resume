{
  "name": "dynamic-resume",
  "version": "1.0.0",
  "description": "Dynamic multi-resume system with schema validation",
  "scripts": {
    "validate": "node scripts/validate-resume.js",
    "validate:watch": "nodemon --watch data --ext json,md --exec 'npm run validate'"
  },
  "devDependencies": {
    "ajv": "^8.12.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
